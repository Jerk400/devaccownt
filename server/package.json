    "app-module-path": "^2.2.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.1.0",
    "enve": "^1.0.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "node-persist": "^3.0.5",
    "nodemailer": "^6.3.0",
    "qrcode": "^1.4.1",
    "qs": "^6.8.0",
    "speakeasy": "^2.0.0"
